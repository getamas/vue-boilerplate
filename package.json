{
  "name": "vue-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Vue2 project boilerplate for large scale applications.",
  "author": "Tamás Gerényi <getamas04@gmail.com> (https://tamasgerenyi.netlify.app)",
  "bugs": {
    "url": "https://github.com/getamas/vue-starter/issues"
  },
  "engines": {
    "node": ">=10.13.3",
    "npm": ">=6"
  },
  "license": "MIT",
  "repository": "https://github.com/getamas/vue-starter",
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch --coverage",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "lint": "vue-cli-service lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|json|vue)\" --write",
    "new": "cross-env HYGEN_TMPLS=generators hygen new"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "npm run lint",
      "npm run test:unit -- --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.8",
    "hygen": "^6.0.4",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "~2.0.8",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
